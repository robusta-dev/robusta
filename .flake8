[flake8]
max-line-length = 120
exclude = .git,
    __pycache__,
    old,
    build,
    dist,
    .venv,
    .vscode,
    .pytest_cache,
    __init__.py,
    .mypy_cache,
    src/robusta/integrations/kubernetes/autogenerated,
    src/robusta/integrations/kubernetes/custom_models.py
ignore = E501, W503, E203
